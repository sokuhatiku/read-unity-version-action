{
  "name": "read-unity-version-action",
  "version": "1.0.0",
  "private": true,
  "description": "A GitHub action to Reads Unity editor version required by project.",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "pack": "ncc build",
    "test": "jest",
    "all": "npm run build && npm run format && npm run lint && npm run pack && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sokuhatiku/read-unity-version-action.git"
  },
  "keywords": [
    "actions",
    "node",
    "unity"
  ],
  "author": "Sokuhatiku",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.9.1"
  },
  "devDependencies": {
    "@types/jest": "^24.9.1",
    "@types/js-yaml": "^3.12.3",
    "@types/node": "^12.12.37",
    "@typescript-eslint/parser": "^2.30.0",
    "@zeit/ncc": "^0.20.5",
    "eslint": "^5.16.0",
    "eslint-plugin-github": "^2.0.0",
    "eslint-plugin-jest": "^22.21.0",
    "jest": "^25.5.1",
    "jest-circus": "^25.5.1",
    "js-yaml": "^3.13.1",
    "prettier": "^1.19.1",
    "ts-jest": "^25.4.0",
    "typescript": "^3.8.3"
  }
}
